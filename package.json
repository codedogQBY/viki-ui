{
  "private": false,
  "name": "viki-ui",
  "version": "0.1.5",
  "description": "A use of the React and typeScript in building a UI component library",
  "author": "qbyqby",
  "license": "MIT",
  "keywords": ["Component","UI","React","viki"],
  "homepage":"https://viki.codedogs.top",
  "repository": {
    "type": "git",
    "url": "https://github.com/codedogQBY/viki-ui"
  },
  "files": ["dist"],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "clean":"rimraf ./dist",
    "lint":"eslint --ext js,ts,tsx src",
    "build": "npm run clean && npm run build-ts && npm run build-sass",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-sass": "node-sass ./src/style/index.scss ./dist/index.css",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "react-transition-group": "^4.4.1"
  },
  "peerDependencies": {
    "react":">=16.8.0",
    "react-dom":">=16.8.0"
  },
  "devDependencies": {
    "typescript": "^4.1.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1",
    "wrappy": "^1.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "babel-plugin-import": "^1.13.3",
    "node-sass": "^4.14.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@types/jest": "^26.0.22",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.13",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "yorkie": "^2.0.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.1",
    "@umijs/plugin-sass": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
